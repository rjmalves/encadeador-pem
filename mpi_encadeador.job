# Variaveis importantes

CASO=`basename $PWD`
WORKDIR=$PWD
DIR_SCRIPTS=~/rotinas/encadeador-pem
ENCADEIA=$DIR_SCRIPTS/main.py

echo Ativando o ambiente virtual
source $DIR_SCRIPTS/venv/bin/activate

